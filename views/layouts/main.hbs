<!DOCTYPE html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js"> <!--<![endif]-->
  <head>
      <meta charset="utf-8" />
      <meta http-equiv="X-UA-Compatible" content="IE=edge" />
      <title>{{title}}</title>
      <meta name="description" content="" />
      <meta name="viewport" content="width=device-width, initial-scale=1" />
      <link rel="dns-prefetch" href="//ajax.googleapis.com">
      {{{block "stylesheets"}}}
  </head>
  <body>

      <!--[if lt IE 7]>
      <p class="browsehappy">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
      <![endif]-->

     {{{body}}}

     {{! Only needs the Handlebars Runtime, because the exposed templates are precompiled. }}
      <script src="http://rawgithub.com/wycats/handlebars.js/1.0.0/dist/handlebars.runtime.js" type='text/javascript'></script>
      {{#if isDev}}
      <script src="//localhost:35729/livereload.js" type='text/javascript'></script>
      {{/if}}
      <script type='text/javascript'>
          console.log({{{json data}}});
          (function () {
              var revive    = Handlebars.template,
                  templates = Handlebars.templates = Handlebars.templates || {};

            {{#templates}}
              templates['{{{name}}}'] = revive({{{template}}});
            {{/templates}}
            //console.log(templates.shared());
          }());
      </script>
      <!-- Google Analytics: change UA-XXXXX-X to be your site's ID. -->
      <script src='/js/lib/google-analytics.js' type='text/javascript'></script>
  </body>
</html>