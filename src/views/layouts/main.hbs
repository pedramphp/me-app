<head>
  <meta charset="utf-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge" />
  <title>{{title}}</title>
  <meta name="description" content="" />
  <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" />
  <link rel="dns-prefetch" href="//ajax.googleapis.com">
  {{{block "stylesheets"}}}
</head>
<body>
      {{{body}}}

      {{! Only needs the Handlebars Runtime, because the exposed templates are precompiled. }}
      <script src="http://rawgithub.com/wycats/handlebars.js/1.0.0/dist/handlebars.runtime.js" type='text/javascript'></script>
      {{#if isDev}}
      <script src="//localhost:35729/livereload.js" type='text/javascript'></script>
      {{/if}}
      <script type='text/javascript'>
        console.log({{{json data}}});
        (function () {
          var revive    = Handlebars.template,
          templates = Handlebars.templates = Handlebars.templates || {};

          {{#templates}}
          templates['{{{name}}}'] = revive({{{template}}});
          {{/templates}}
            //console.log(templates.shared());
          }());
      </script>
      <script src='js/lib/jquery-1.10.2.min.js'></script>

      {{{block "js-bottom"}}}
      <script src='/js/lib/google-analytics.js' type='text/javascript'></script>

    </body>
    </html>